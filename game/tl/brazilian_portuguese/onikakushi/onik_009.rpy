    # nvl clear
    # voice "voice/ps3/s19/02/500200913.ogg"
    # a_rena "\"This way, all the evil that possesses you is sucked out by the cotton."
    nvl clear
    voice "voice/ps3/s19/02/500200913.ogg"
    a_rena "\"Deste jeito, todo o mal que você possui é sugado para fora pelo algodão."

# game/onikakushi/onik_009.rpy:2456
translate brazilian_portuguese onik_009_e92ba4d6:

    # voice "voice/ps3/s19/02/500200914.ogg"
    # extend " ......Then you let it gently drift away on the stream, and you're done.\""
    voice "voice/ps3/s19/02/500200914.ogg"
    extend " ......Então você gentilmente o afunda na água, e está pronto.\""

# game/onikakushi/onik_009.rpy:2458
translate brazilian_portuguese onik_009_14f17956:

    # n "Together, Rena and I set our pieces of cotton afloat on the surface of the water."
    n "Juntos, Rena e eu colocamos nossos pedaços de algodão flutuando na superfície da água."

# game/onikakushi/onik_009.rpy:2460
